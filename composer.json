{
    "name": "ddme/craft-hub-plugin",
    "description": "Monitor your Craft CMS sites from a single dashboard.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "ddme\\crafthub\\": "src/"
        }
    },
    "extra": {
        "name": "Craft Hub",
        "handle": "crafthub",
        "hasCpSettings": true,
        "hasCpSection": false,
        "class": "ddme\\crafthub\\CraftHub"
    }
}
